<PluginOutlet @name="above-right-sidebar-blocks" />

{{#each this.blocks as |block|}}
  <div class={{block.classNames}}  {{#if block.campaign_id}}
         {{on "click" (fn this.handleBlockClick block) capture=true}}
       {{/if}}>
    {{component block.internalName params=block.parsedParams}}
  </div>
  <PluginOutlet
    @name="below-right-sidebar-block"
    @outletArgs={{hash block=block}}
  />
{{/each}}

<PluginOutlet @name="below-right-sidebar-blocks" />